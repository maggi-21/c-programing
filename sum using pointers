#include<stdio.h>
int main() {
   int *p;
   int i,n,sum=0;
   printf("enter a number");
   scanf("%d",&n);
   for(i=0;i<n;i++)
   {
       scanf("%d",(p+i));
   }
   for(i=0;i<n;i++)
   {
       sum=sum+*(p+i);
   }
   printf("%d",sum);
    return 0;
}
output-1:
enter a number3
1 4 6
11
output-2:
enter a number4
1 1 2 3
7
