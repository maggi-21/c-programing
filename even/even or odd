#include <stdio.h>
int check(int x);
void main() {
    int x,ans;
    printf("enter a number");
    scanf("%d",&x);
   ans=check(x);
    if(ans==0)
    printf("Even");
    else
    printf("odd");
}
int check(int x)
{
    if(x%2==0)
    return 0;
    else
    return 1;
}
output:
enter a number6
Even
